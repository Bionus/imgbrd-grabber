<?xml version="1.0" encoding="utf-8" ?>
<Site>
	<Name>Metabooru</Name>
	<Urls>
		<Html>
			<Home>/?position={page}</Home>
			<Tags>/tags/{tags}?position={page}</Tags>
			<Post>/{id}</Post>
			<Image>/thumb200-></Image>
		</Html>
	</Urls>
	<Regex>
		<Tags>&lt;a class=&quot;inline-tag tag-type-([^&quot;]+)&quot; href=&quot;http://metabooru.com/tags/[^&quot;]+&quot;&gt;&lt;img [^&gt;]*&gt; ([^&lt;]+)&lt;/a&gt;</Tags>
		<Image>&lt;a class=&quot;link&quot; href=&quot;/([0-9]+)&quot;&gt;&lt;img class=&quot;[^&quot;]*&quot; src=&quot;(http://[^.]+\.metabooru\.com/([^/]+)/thumb200)&quot; alt=&quot;[^&quot;]*&quot; style=&quot;[^&quot;]*&quot;/&gt;&lt;/a&gt;&lt;/div&gt;&lt;div class=&quot;score&quot;&gt;&lt;a [^&gt;]*&gt;&lt;span class=&quot;icon&quot;&gt;&lt;/span&gt;&lt;/a&gt;&lt;a [^&gt;]*&gt;&lt;span class=&quot;icon&quot;&gt;&lt;/span&gt;&lt;/a&gt;&lt;div class=&quot;value&quot;&gt;([0-9]+)%&lt;/div&gt;&lt;/div&gt;&lt;div class=&quot;tags&quot;&gt;&lt;a href=&quot;http://metabooru.com/tags/[^&quot;]*&quot; title=&quot;([^&quot;]*)&quot;&gt;&lt;img [^&gt;]*/&gt;&lt;/a&gt;</Image>
		<Order>id|preview_url|md5|score|tags</Order>
		<ImageTags>&lt;span class=&quot;name tag-type-([^&quot;']+)&quot;&gt;([^&lt;]+)&lt;/span&gt;</ImageTags>
	</Regex>
	<FirstPage>0</FirstPage>
</Site>