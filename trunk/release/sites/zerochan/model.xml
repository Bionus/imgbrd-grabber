<?xml version="1.0" encoding="utf-8" ?>
<Site>
	<Name>Zerochan</Name>
	<Urls>
		<Html>
			<Tags>/{tags}?p={page}</Tags>
			<Post>/{id}</Post>
			<Image>/600/->/full/</Image>
		</Html>
		<Rss>
			<Tags>/{tags}?xml&amp;p={page}</Tags>
			<Limit>100</Limit>
			<Image>/600/->/full/</Image>
		</Rss>
	</Urls>
	<Regex>
		<Tags>&lt;li class=&quot;tag-type-(.+)&quot;&gt;.*&lt;a href=&quot;.+&quot;&gt;.*&lt;/a&gt;.*&lt;a href=&quot;.+&quot;&gt;(.+)&lt;/a&gt;.*&lt;/li&gt;</Tags>
		<Image>&lt;a href='/post/view/([^']+)'[^&gt;]*&gt;[^&lt;]*(&lt;img(?: id='(?:[^']*)')? title='(.+) // (.+)x(.+) // [^']*' alt='[^']*'(?: height='[^']*')? width='[^']*'(?: height='[^']*')? src='.*(/_thumbs/([^/]*)/thumb.jpg)' ?/?&gt;)[^&lt;]*&lt;/a&gt;</Image>
		<Order>id|image|tags|width|height|preview_url|md5</Order>
		<Count>has ([0-9,]+) .*images\.</Count>
	</Regex>
	<FirstPage>1</FirstPage>
</Site>