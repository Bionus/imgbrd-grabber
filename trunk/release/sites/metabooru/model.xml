<?xml version="1.0" encoding="utf-8" ?>
<Site>
	<Name>Metabooru</Name>
	<Urls>
		<Html>
			<Home>/?page={page}</Home>
			<Tags>/m/{tags}?page={page}</Tags>
			<Post>/m/{tags}/{id}</Post>
			<Image>http://content.metabooru.com/{md5}</Image>
		</Html>
	</Urls>
	<Regex>
		<Tags>&lt;a class=&quot;([^&quot;]+)&quot; (?:href|id)=&quot;(?:[^&quot;]+)&quot; title=&quot;([^&quot;]+)&quot; (?:[^&gt;]+)&gt;</Tags>
		<Image>&lt;article class=&quot;(?:[^&quot;]+)&quot;&gt;(?:&lt;a (?:[^&gt;]+)&gt;(?:[^&lt;]*)&lt;span class=&quot;icon(?: unread)?&quot;&gt;&lt;/span&gt;&lt;/a&gt;)?&lt;a (?:[^&gt;]+)&gt;&lt;span class=&quot;label&quot;&gt;([^&quot;]+)&lt;/span&gt;&lt;/a&gt;&lt;div class=&quot;score&quot;&gt;&lt;form (?:[^&gt;]+)&gt;&lt;input (?:[^&gt;]+)&gt;&lt;input (?:[^&gt;]+)&gt;&lt;input (?:[^&gt;]+)&gt;&lt;input (?:[^&gt;]+)&gt;&lt;/form&gt;&lt;form (?:[^&gt;]+)&gt;&lt;input (?:[^&gt;]+)&gt;&lt;input (?:[^&gt;]+)&gt;&lt;input (?:[^&gt;]+)&gt;&lt;input (?:[^&gt;]+)&gt;&lt;/form&gt;&lt;span (?:[^&gt;]+)&gt;&lt;span&gt;(\d+)&amp;#(?:\d+);&lt;/span&gt;&lt;/span&gt;&lt;/div&gt;&lt;a class=&quot;thumb&quot; title=&quot;Post #(\d+) (?:[^&quot;]+)&quot; (?:[^&gt;]+)&gt;(&lt;img[^&gt;]+ src=&quot;(http://(?:[^.]+).metabooru.com/([^/]+)/thumb200)&quot; width=&quot;([^&quot;]+)&quot; height=&quot;([^&quot;]+)&quot; /&gt;)&lt;/a&gt;&lt;/article&gt;</Image>
		<Order>tags|rating|id|image|preview_url|md5|width|height</Order>
	</Regex>
	<FirstPage>1</FirstPage>
</Site>